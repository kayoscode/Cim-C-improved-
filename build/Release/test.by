
int main(int argv, const char** argv) {
	for(int i = 0; i <= 100; i++) {
		bool either = false;

		if(i % 5 == 0) {
			printf("fizz");
			either = true;
		}
		if(i % 3) {
			printf("buzz");
			either = true;
		}
		if(either) {
			printf("\n");
		}
	}

	return 0;
}

class Math {
	public static int add(int a, int b) return a + b;
	public static int sub(int a, int b) return a - b;
	public static int mul(int a, int b) return a * b;

	public static int div(int a, int b, int* mod) {
		*mod = a % b;
		return a / b;
	}
}